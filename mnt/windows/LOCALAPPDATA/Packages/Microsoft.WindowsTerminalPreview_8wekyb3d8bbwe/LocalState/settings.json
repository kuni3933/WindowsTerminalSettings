{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "commandPalette"
      },
      "keys": "ctrl+shift+p"
    },
    {
      "command": "unbound",
      "keys": "ctrl+comma"
    },
    {
      "command": "unbound",
      "keys": "ctrl+v"
    },
    {
      "command": "paste",
      "keys": "ctrl+shift+v"
    },
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+shift+c"
    },
    {
      "command": "scrollUpPage",
      "keys": "shift+pgup"
    },
    {
      "command": {
        "action": "openSettings"
      },
      "keys": "ctrl+shift+s"
    },
    {
      "command": {
        "action": "scrollUp"
      },
      "keys": "shift+up"
    },
    {
      "command": "find",
      "keys": "ctrl+alt+f"
    },
    {
      "command": {
        "action": "scrollDown"
      },
      "keys": "shift+down"
    },
    {
      "command": "scrollDownPage",
      "keys": "shift+pgdn"
    },
    {
      "command": {
        "action": "newTab"
      },
      "keys": "ctrl+t"
    },
    {
      "command": {
        "action": "closeTab"
      },
      "keys": "alt+w"
    },
    {
      "command": {
        "action": "nextTab"
      },
      "keys": "alt+l"
    },
    {
      "command": {
        "action": "prevTab"
      },
      "keys": "alt+h"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "down"
      },
      "keys": "ctrl+alt+j"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "right"
      },
      "keys": "ctrl+alt+l"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "left"
      },
      "keys": "ctrl+alt+h"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "up"
      },
      "keys": "ctrl+alt+k"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "left"
      },
      "keys": "alt+shift+h"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "down"
      },
      "keys": "alt+shift+j"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "right"
      },
      "keys": "alt+shift+l"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "up"
      },
      "keys": "alt+shift+k"
    }
  ],
  "experimental.autoMarkPrompts": true,
  "experimental.showMarksOnScrollbar": true,
  "experimental.rendering.forceFullRepaint": true,
  "experimental.rendering.software": true,
  "experimental.useBackgroundImageForWindow": false,
  "copyFormatting": "none",
  "copyOnSelect": false,
  "launchMode": "default",
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "disabledProfileSources": ["Windows.Terminal.Azure"],
  "useAcrylicInTabRow": true,
  "profiles": {
    "defaults": {
      "antialiasingMode": "cleartype",
      "backgroundImageOpacity": 0.1,
      "backgroundImageStretchMode": "uniformToFill",
      "bellStyle": "audible",
      "experimental.useAtlasEngine": true,
      "closeOnExit": "always",
      "colorScheme": "Iceberg",
      "cursorColor": "#FFFFFF",
      "cursorShape": "bar",
      "font": {
        "face": "PlemolJP Console NF",
        "size": 12,
        "weight": "normal"
      },
      "historySize": 9001,
      "opacity": 85,
      "padding": "0, 0, 0, 0",
      "scrollbarState": "visible",
      "snapOnInput": true,
      "startingDirectory": "%USERPROFILE%",
      "useAcrylic": true
    },
